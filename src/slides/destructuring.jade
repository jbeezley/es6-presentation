section
    h3 Destructuring
    pre
        code.
            // arrays
            var [a, , b] = [1, 2, 3];

            // objects
            var {a, b} = {a: 1, b: 3, c: 2};
            var {a: d} = {a: 1}  // d === 1

            // swap
            [a, b] = [b, a];

            // defaults
            var [a = 1] = [];  // a === 1

            // function parameters
            function f({name, shape, value}) {
                return name;
            }
            f({name: 'Joe'});  // -> 'Joe'

            function f(obj) {
                var name = obj.name;
            }

